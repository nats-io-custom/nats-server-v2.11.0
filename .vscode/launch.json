    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "nats server - StaticConf",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/nats-server",
            "console": "externalTerminal",
            "env": {
                "SERVER_NAME":"nats"
            },
            "args": [ 
                "-n","internal-server",
                "-m","8222",
                "-js",
                "-debug",
                "-c","${workspaceFolder}/configs/static_callout.conf",

            ]    
        }, {
            "name": "nats server - StaticConf-Lookup",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/nats-server",
            "console": "externalTerminal",
            "env": {
                "SERVER_NAME":"nats"
            },
            "args": [ 
                "-n","internal-server",
                "-m","8222",
                "-js",
                "-debug",
                "-c","${workspaceFolder}/configs/static_callout_lookup.conf",

            ]    
        }
    ]
}